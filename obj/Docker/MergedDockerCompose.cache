services:
  votingapi.webapi:
    build:
      context: D:\WorkSpace\.Net Core\VotingAppAPI
      dockerfile: Presentation/VotingAPI.WebAPI/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:80
    image: votingapiwebapi
    ports:
    - published: 5000
      target: 80
    - target: 80
    volumes:
    - C:\Users\omer\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\omer\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'