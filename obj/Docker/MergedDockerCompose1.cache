services:
  votingapi.webapi:
    build:
      context: D:\WorkSpace\.Net Core\VotingAppAPI
      dockerfile: Presentation/VotingAPI.WebAPI/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: VotingAPI.WebAPI
      target: base
    container_name: VotingAPI.WebAPI
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: votingapiwebapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5000
      target: 80
    - target: 80
    tty: true
    volumes:
    - D:\WorkSpace\.Net Core\VotingAppAPI\Presentation\VotingAPI.WebAPI:/app:rw
    - C:\Users\omer\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\omer\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\omer\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\omer\.nuget\packages:/root/.nuget/packages:ro
    - D:\WorkSpace\.Net Core\VotingAppAPI:/src:rw
version: '3.4'